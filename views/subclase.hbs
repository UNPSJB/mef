<div style="padding-top: 10px"></div>
<h1>Subclases  <a href="/dinosaurios/agregarDino" class="btn btn-primary" rel="noopener noreferrer">+</a></h1>
<table class="table">
        <thead>
            <th scope="col">Nombre</th>
            <th scope="col">Clase</th>
            <th scope="col">Acciones</th>
        </thead>
        <tbody>
        {{#each result}}
            <tr>
                <td>{{this.nombre}}</td>
                <td>{{this.clase}}</td>
                <!-- acciones aca -->
                <td>
                    <a href="/dinosaurios/editarDino?id={{this.id}}"><i class="far fa-edit"></i></a> <!--editar-->
                    <a href="/dinosaurios/eliminarDino?id={{this.id}}"><i class="fas fa-minus-circle"></i></a>
                {{!-- otras acciones mas --}}
                </td>
            </tr>
        {{/each}}

        </tbody>    
    </table>
</div>