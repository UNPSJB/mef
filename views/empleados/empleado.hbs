<div style="padding-top: 10px">
  <h1>Empleados <a href="/empleados/agregar" class="btn btn-success btn-primary" rel="noopener noreferrer">+</a></h1>
  <table class="table table-striped table-bordered" >
    <thead>
      <th scope="col">Documento</th>
      <th scope="col">Nombre</th>
      <th scope="col">Apellido</th>
      <th scope="col">Direccion</th>
      <th scope="col">Localidad</th>
      <th scope="col">Email</th>
      <th scope="col">Fecha de Nacimiento</th>
      <th scope="col">Telefono</th>
      <th scope="col">Acciones</th>
    </thead>
    <tbody>
      {{#each results}}
      <tr>
        <td>{{this.Persona.identificacion}}</td>
        <td>{{this.Persona.nombre}}</td>
        <td>{{this.Persona.apellido}}</td>
        <td>{{this.Persona.direccion}}</td>
        <td>{{this.Persona.localidad}}</td>
        <td>{{this.Persona.email}}</td>
        <td>{{this.Persona.fecha_nacimiento}}</td>
        <td>{{this.Persona.telefono}}</td>
        <!-- acciones aca -->
        <td>
          <a href="/empleados/editar/{{this.id}}">
            <span class="material-icons">edit</span>
          </a>
          <!--editar-->
          <a href="/empleados/eliminar/{{this.id}}">
            <span style="color:red" class="material-icons">delete</span>
          </a>
          {{!-- otras acciones mas --}}
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  {{> pagination/footer paginationObj }}
</div>